<div class="row">
  <div class="col">
    <h1> Users </h1>
  </div>
</div>
<div class="row">
  <div class="col">
    <div class="table-responsive">
      <table class="table">
        <thead>
          <th>Role</th>
          <th class="text-right">Edit User</th>
        </thead>
        <tbody>
          <% @roles.each do |role| %>
            <tr>
              <td><%= role.role %></td>
              <td class="text-right"><a href=" <%= edit_admin_role_path(role) %>" class="btn btn-primary"> Edit </a></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <td class="text-right"><a href=" <%= new_admin_role_path %>" class="btn btn-primary"> Add Role xo </a></td>
    </div>
  </div>
</div>
